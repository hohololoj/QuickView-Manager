{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "quick.view.manager",
  "version": "1.0.0",
  "defaultMode": "window",
  "logging": {"enabled": false},
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "nativeAllowList": [
    "app.*",
    "os.updateSpawnedProcess",
    "os.spawnProcess",
    "os.showMessageBox",
    "os.getPath",
    "os.setTray",
    "filesystem.createDirectory",
    "filesystem.writeFile",
    "filesystem.readFile",
    "filesystem.getStats",
    "resources.extractFile",
    "window.show",
    "window.hide",
    "window.setIcon",
    "window.focus"
  ],
  "modes": {
    "window": {
      "title": "QuickView Manager",
      "width": 800,
      "height": 600,
      "minWidth": 800,
      "minHeight": 600,
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/resources/icons/icon.ico",
      "enableInspector": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": false,
      "exitProcessOnClose": false
    }
  },
  "cli": {
    "binaryName": "QuickViewManager",
    "resourcesPath": "/resources/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "6.3.0",
    "clientVersion": "6.3.0"
  }
}